{% block content %}
    <section class="hero-wrap hero-wrap-2" style="background-image: url('{{  asset('assets/images/') ~ articlex.image }}');">
        <div class="overlay"></div>
        <div class="container">
            <div class="row no-gutters slider-text align-items-center justify-content-center">
                <div class="col-md-9 ftco-animate text-center fadeInUp ftco-animated">
                    <h1 class="mb-2 bread">{{ articlex.titre}}</h1>
                    <p class="breadcrumbs"><span class="mr-2"><a href="##">Accueil <i class="ion-ios-arrow-forward"></i></a></span> <span class="mr-2"><a href="{{ path('Blog_Homepage_User') }}">Blog <i class="ion-ios-arrow-forward"></i></a></span> <span>{{ articlex.titre }} <i class="ion-ios-arrow-forward"></i></span></p>
                    <p class="breadcrumbs"><span class="mr-2" style="background-color: #4b4bff;border-radius: 9px;">{{ categorie.nom }}</span></p>

                </div>
            </div>
        </div>
    </section>



    <section class="ftco-section" style="padding: 24px;">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 ftco-animate fadeInUp ftco-animated">

                    <p>{{ articlex.contenu|raw }}</p>
                    <div class="sharelink-share-buttons-container">
                        <ul class="sharelink-share-buttons">
                            <li><a href="https://www.facebook.com/sharer/sharer.php?u=http://127.0.0.1:8000/blog/{{ articlex.id }}" class="share-link facebook" target="_blank"><i class="fa fa-facebook"></i><span class="textshare">Partagez</span></a></li>
                            <li><a href="https://twitter.com/share?url=http://127.0.0.1:8000/blog/{{ articlex.id }}" class="share-link twitter" target="_blank"><i class="fa fa-twitter"></i><span class="textshare">Tweetez</span></a></li>
                            <li><a href="https://www.linkedin.com/shareArticle?mini=true&amp;url=http://127.0.0.1:8000/blog/{{ articlex.id }}" class="share-link linkedin" target="_blank"><i class="fa fa-linkedin"></i><span class="textshare">Partagez</span></a></li>
                            <li><a href="https://pinterest.com/pin/create/button/?url=http://127.0.0.1:8000/blog/{{ articlex.id }}" class="share-link pinterest" target="_blank"><i class="fa fa-pinterest"></i><span class="textshare">Sauvegarder</span></a></li>
                            <li><a href="mailto:info@websiteplanet.com&amp;subject=http://127.0.0.1:8000/blog/{{ articlex.id }}&amp;body=http://127.0.0.1:8000/blog/{{ articlex.id }}" class="share-link email" target="_blank"><i class="fa fa-mail"></i><span class="textshare">E-MAIL</span></a></li>
                        </ul>
                    </div>
                    <div class="tag-widget post-tag-container mb-5 mt-5">
                        <div class="tagcloud">
                          {% for tagsofarti in tagsofarticle %}
                            <a href="{{ path('Affiche_tag_User',{'id':tagsofarti.id})}}" class="tag-cloud-link">{{ tagsofarti.nom }}</a>
                            {% endfor %}
                        </div>
                    </div>





                </div> <!-- .col-md-8 -->

                <div class="col-lg-4 sidebar ftco-animate fadeInUp ftco-animated">

                    <div class="sidebar-box ftco-animate fadeInUp ftco-animated" style="margin-bottom: -14px;">
                        <div style="border: 1px solid;height: 31px;border-radius: 15px;"><h3 style="margin-left: 77px;color: green;font-weight: bold;margin-top: 5px;"> Nombre de vues : {{ nbrvue }}</h3></div>

                    </div>
                    <div class="sidebar-box ftco-animate fadeInUp ftco-animated" style="margin-top: 40px;">
                        <h3 style="border-bottom: 1px solid;"> Articles Populaire :</h3>

                        <ul class="categories">
                            {% for artipop in articles_populaires %}
                            <li><a href="{{ path('Single_Affiche_article_User',{'id':artipop.id}) }}">{{ artipop.titre }} </a></li>
                            {% endfor %}
                         </ul>
                    </div>
                    <div class="sidebar-box ftco-animate fadeInUp ftco-animated">
                        <h3>Categories</h3>
                        <ul class="categories">
                            {% for categoris in categories %}
                            <li><a href="{{ path('Affiche_categorie_User',{'id':categoris.id})}}">{{ categoris.nom }} </a></li>
                            {% endfor %}
                        </ul>
                    </div>
                    <div class="sidebar-box ftco-animate fadeInUp ftco-animated" style="margin-top: 40px;">
                        <h3 style="border-bottom: 1px solid;"> Articles Similaires :</h3>

                        <ul class="categories">
                            {% for artipop in articles_similaires %}
                                <li><a href="{{ path('Single_Affiche_article_User',{'id':artipop.id}) }}">{{ artipop.titre }} </a></li>
                            {% endfor %}
                        </ul>
                    </div>


                    <div class="sidebar-box ftco-animate fadeInUp ftco-animated">
                        <h3>Tag Cloud</h3>
                        <ul class="tagcloud m-0 p-0">
                            {% for alltag in alltags %}
                                <a href="{{ path('Affiche_tag_User',{'id':alltag.id})}}" class="tag-cloud-link">{{ alltag.nom }}</a>
                            {% endfor %}
                        </ul>
                    </div>


                </div><!-- END COL -->
            </div>
        </div>
    </section>




{% endblock %}
